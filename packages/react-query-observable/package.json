{
  "name": "@valian/react-query-observable",
  "description": "react query rxjs observable",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valian-ca/react-firebase.git"
  },
  "author": {
    "name": "Valian",
